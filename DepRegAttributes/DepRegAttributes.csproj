<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net7.0</TargetFrameworks>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageId>DBaker.$(AssemblyName)</PackageId>
		<Authors>dalton_s_baker</Authors>
		<Company />
		<Copyright></Copyright>
		<Description>A handy package to register dependencies with attributes. Never touch your Program.cs file again!</Description>
		<PackageProjectUrl>https://github.com/dalton-baker/DepRegAttributes</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/dalton-baker/DepRegAttributes</RepositoryUrl>
		<RepositoryType>github</RepositoryType>
		<PackageTags>DependancyInjection, DI, IServiceCollection, IServiceProvider</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>5.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.32" />
	</ItemGroup>
</Project>
